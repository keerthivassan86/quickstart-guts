{
    "builders": [
        {
            "type": "openstack",
            "ssh_username": "{{ user `ssh_username` }}",
            "image_name": "{{ user `image_name` }}",
            "source_image_name": "{{ user `source_image_name` }}",
            "flavor": 2,
            "networks": [
                "{{ user `network_alok` }}"
            ],
            "floating_ip": "{{ user `floating_ip_181` }}"
        }
    ],
    "provisioners": [
        {
            "type": "shell",
            "scripts": [
                "scripts/customize.sh",
                "scripts/cleanup.sh",
                "scripts/zerodisk.sh"
            ]
        }
    ]
}
